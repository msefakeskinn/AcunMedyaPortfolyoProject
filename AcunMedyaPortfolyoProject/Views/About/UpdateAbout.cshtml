@model AcunMedyaPortfolyoProject.Models.TableAbout
@{
    ViewBag.Title = "UpdateAbout";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Burası Hkkımızda Güncelleme Sayfası</h2>
<form method="post">
    @Html.HiddenFor(a => a.AboutID)
    <label>Resim</label>
    @Html.TextBoxFor(x => x.ImageURL, new { @class = "form-control" })
    <label>Başlık</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <label>Doğum</label>
    @Html.TextBoxFor(x => x.BirthDay, new { @class = "form-control" })
    <label>Website</label>
    @Html.TextBoxFor(x => x.WebSite, new { @class = "form-control" })
    <label>Telefon</label>
    @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })
    <label>Şehir</label>
    @Html.TextBoxFor(x => x.City, new { @class = "form-control" })
    <label>Yaş</label>
    @Html.TextBoxFor(x => x.Age, new { @class = "form-control" })
    <label>Mail</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
    <label>Freelance</label>
    @Html.TextBoxFor(x => x.Freelance, new { @class = "form-control" })
    <label>Açıklama 1</label>
    @Html.TextBoxFor(x => x.Description1, new { @class = "form-control" })
    <label>Açıklama 2</label>
    @Html.TextBoxFor(x => x.Description2, new { @class = "form-control" })
    <label>Degree</label>
    @Html.TextBoxFor(x => x.Degree, new { @class = "form-control" })
    <button>Güncelle</button>
    </form>


