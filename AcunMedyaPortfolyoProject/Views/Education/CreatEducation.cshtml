
@{
    ViewBag.Title = "CreatEducation";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Eğitim Oluşturma Sayfası</h2>
<form method="post">
    <label>Başlangıç Yılı</label>
    <input type="text" class="form-control" name="StartYear" />
    <label>Bitiş Yılı</label>
    <input type="text" class="form-control" name="EndYear" />
    <label>Okul Adı</label>
    <input type="text" class="form-control" name="Name" />
    <label>Açıklama</label>
    <input type="text" class="form-control" name="Description" />
    <label>Bölüm</label>
    <input type="text" class="form-control" name="Section" />
    <button class="btn btn-success">Ekle</button>
</form>